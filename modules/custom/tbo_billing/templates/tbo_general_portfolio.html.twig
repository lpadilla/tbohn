{% macro one_column(label, value, class) %}
  {% import _self as forms %}
  <div class="input-field content-row col m1 s6 l1">
    {% if value.select_multiple_ng_options %}
      {{ forms.select_multiple_ng_options(label, value) }}
    {% elseif value.autocomplete_portafolio %}
      {{ forms.autocomplete_portafolio(label, value) }}
    {% elseif value.logo_filter %}
      {{ forms.logo_filters(label, value) }}
    {% else %}
      {% if value.validate_length %}
        <input id="{{ value.identifier }}" data-ng-model="{{ value.identifier }}"
               class="form-control format-moments2" type="{{ value.input_type }}" {{ value.validations }}
               maxlength="{{ value.validate_length }}"/>
        <label for="{{ value.identifier }}" class="">{{ label }}:</label>
      {% elseif (value.value is defined) and (value.disabled is defined) %}
        <input value="{{ value.value }}"
               class="form-control format-moments2" type="{{ value.input_type }}" {{ value.validations }} {{ value.disabled }} />
        <label for="{{ value.identifier }}" class="">{{ label }}:</label>
      {% else %}
        <input id="{{ value.identifier }}" data-ng-model="{{ value.identifier }}"
               class="form-control format-moments2" type="{{ value.input_type }}" {{ value.validations }} />
        <label for="{{ value.identifier }}" class="">{{ label }}:</label>
      {% endif %}
    {% endif %}
  </div>
{% endmacro %}

