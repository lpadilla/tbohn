<!-- Select with ng-options -->
{% macro select_multiple_ng_options(label, value) %}
  <div class="form-item form-group form-type-select form-item-billing-select-enterprise">
    {% if value.identifier == 'user' %}
      <select id="billing-select-category" name="billing-select-category" ng-model="selectedUser" ng-change='changeUser()' class="" material-select multiple watch>
        <option value="" selected disabled>{[{ labelUser }]}</option>
        <option value="" selected disabled>{{ 'Todas' }}</option>
        <option ng-if="quantiyUser == 1" ng-repeat="option in user" value="{[{option.name}]}" selected disabled>{[{option.name}]}</option>
        <option ng-if="quantiyUser > 1" ng-repeat="option in user" value="{[{option.name}]}">{[{option.name}]}</option>
      </select>
    {% elseif value.identifier == 'request_code' %}
      <select id="billing-select-category" name="billing-select-category" ng-model="selectedRequestCode" ng-change='changeRequestCode()' class="" material-select multiple watch>
        <option value="" selected disabled>{[{ labelRequestCode }]}</option>
        <option value="" selected disabled>{{ 'Todas' }}</option>
        <option ng-if="quantityRequestCode == 1" ng-repeat="option in requestCode" value="{[{option.name}]}" selected disabled>{[{option.name}]}</option>
        <option ng-if="quantityRequestCode > 1" ng-repeat="option in requestCode" value="{[{option.name}]}">{[{option.name}]}</option>
      </select>
    {% elseif value.identifier == 'status' %}
      <select id="billing-select-status" name="billing-select-status" ng-model="selectedStatus" ng-change='changeStatus()' class="" material-select multiple watch>
        <option value="" selected disabled>{[{ labelStatus }]}</option>
        <option value="" selected disabled>{{ 'Todas' }}</option>
        <option ng-if="quantityStatus == 1" ng-repeat="option in status" value="{[{option.name}]}" selected disabled>{[{option.name}]}</option>
        <option ng-if="quantityStatus > 1" ng-repeat="option in status" value="{[{option.name}]}">{[{option.name}]}</option>
      </select>
    {% elseif value.identifier == 'order_number' %}
      <select id="billing-select-order-number" name="billing-select-order-number" ng-model="selectedOrderNumber" ng-change='changeStatus()' class="" material-select multiple watch>
        <option value="" selected disabled>{[{ labelOrderNumber }]}</option>
        <option value="" selected disabled>{{ 'Todas' }}</option>
        <option ng-if="quantityOrderNumber == 1" ng-repeat="option in orderNumber" value="{[{option.name}]}" selected disabled>{[{option.name}]}</option>
        <option ng-if="quantityOrderNumber > 1" ng-repeat="option in orderNumber" value="{[{option.name}]}">{[{option.name}]}</option>
      </select>
    {% elseif value.identifier == 'line_number' %}
      <select id="billing-select-line-number" name="billing-select-line-number" ng-model="selectedLineNumber" ng-change='changeStatus()' class="" material-select multiple watch>
        <option value="" selected disabled>{[{ labelLineNumber }]}</option>
        <option value="" selected disabled>{{ 'Todas' }}</option>
        <option ng-if="quantityLineNumber == 1" ng-repeat="option in lineNumber" value="{[{option.name}]}" selected disabled>{[{option.name}]}</option>
        <option ng-if="quantityLineNumber > 1" ng-repeat="option in lineNumber" value="{[{option.name}]}">{[{option.name}]}</option>
      </select>
    {% else %}
      <select id="billing-select-category" name="billing-select-category" ng-model="selectedCategory" ng-change='changeCategory()' class="" material-select multiple watch>
        <option value="" selected disabled>{[{ labelCategory }]}</option>
        <option value="" selected disabled>Todas</option>
        <option ng-if="quantiyCategory == 1" ng-repeat="option in category" value="{[{option.name}]}" selected disabled>{[{option.name}]}</option>
        <option ng-if="quantiyCategory > 1" ng-repeat="option in category" value="{[{option.name}]}">{[{option.name}]}</option>
      </select>
    {% endif %}
  </div>
{% endmacro %}

<!-- Select with ng-options -->
{% macro autocomplete_portafolio(label, value) %}
  <i class="prefix icon-search-cyan">
    <span class="path1"></span>
  </i>
  <div class="tags-wrapper" ng-mouseleave="closeSuggestions()">
      <div id="tagsList" class="tags-cloud">
        {% if value.validate_length %}
          <input id="{{ value.identifier }}" data-ng-model="{{ value.identifier }}"
                 class="form-control format-moments2"
                 ng-click="searchAutocompleteClick('{{ value.identifier }}')"
                 ng-change="searchAutocomplete('{{ value.identifier }}')" type="text" autocomplete="off"
                 ng-keydown="checkKeyDownReference($event, '{{ value.identifier }}')"
                 maxlength="{{ value.validate_length }}"/>
          <label for="{{ value.identifier }}" class="">{{ label }}</label>
        {% else %}
          <input id="{{ value.identifier }}" data-ng-model="{{ value.identifier }}"
                 class="form-control format-moments2"
                 ng-click="searchAutocompleteClick('{{ value.identifier }}')"
                 ng-change="searchAutocomplete('{{ value.identifier }}')" type="text" autocomplete="off"
                 ng-keydown="checkKeyDownReference($event, '{{ value.identifier }}')"/>
          <label for="{{ value.identifier }}" class="">{{ label }}</label>
        {% endif %}
      </div>
      <div id="suggestions" class="collection" style="border: none;">
        <div id="suggestionsAutocomplete" class="collections suggestions">
          <ul>
            <li class="collection-item" ng-repeat="suggestion in suggestionsAutocomplete"
                ng-click="resultClickedAutocomplete($index, '{{ value.identifier }}')"
                ng-mouseover="$parent.selectedIndexAutocomplete=$index"
                ng-class="{active : selectedIndexAutocomplete===$index}"
                >
              {[{suggestion.name}]}
            </li>
           </ul>
         </div>
      </div>
    </div>
{% endmacro %}

