                  <span class="path1"></span>
                  <span class="path2"></span>
                </i>
              {% endif %}

              <!--Input password-->
              <input id="{{ value.identifier }}" placeholder="{{value.placeholder}}"  data-ng-model="{{ value.identifier }}"
                     class="form-control format-moments2" type="{{ value.input_type }}"
                     {% if value.ngdisabled %} ng-disabled="state" {% endif %}
                     {% if  value.validations %} {{ value.validations }} {% endif %}
                     {% if (value.value is defined) and (value.disabled is defined) %}
                       value="{{ value.value }}" {{ value.disabled }}
                     {% endif %}
                     {% if data.ng_component %} {{ data.ng_component.ng }}="{{ data.ng_component.value }}" {% endif %}
                     {% if value.max_length %} maxlength="{{ value.max_length }}" {% endif %}
                     {% if data.pattern %} pattern="{{ data.pattern }}" title="{{ data.title }}" {% endif %}
                     {% if value.regex %} reg-ex-input {% endif %}
                     {% if value.attributes %}
                       {% for key, value in value.attributes %}
                         {{ key }}="{{ value }}"
                       {% endfor %}
                     {% endif %}
                      />
              <label for="{{ value.identifier }}" class="{{class}}">{{ label }}</label>

              <!--Password force text-->
              {% if data.with_pass_force %}
                <a id="force">{[{ passForce }]}</a>
              {% endif %}

