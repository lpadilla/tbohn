  <div class="input-field  {{label}}{{class}} {% if value.date_line == 1 %}{{ 'form-type-date ' }}{% endif %} content-row col m1 s12">
    {% if value.autocomplete %}
      {{ forms.autocomplete(label, value) }}
    {% elseif value.autocomplete_invoice %}
      {{ forms.autocomplete_invoice(label, value) }}
    {% elseif value.select_multiple %}
      {{ forms.select_multiple(label, value) }}
    {% elseif value.checkboxes_type %}
      {{ value.checkboxes_type }}
    {% elseif value.created %}
      <div class="date-tbo">{{ value.created }}</div>
    {% elseif value.type == "selectable_list" %}
      {{ forms.select_multiple(label, value) }}
    {% elseif value.type == "selectable" or value.type == "select" %}
      {{ forms.select(label, value) }}
    {% elseif value.select_multiple_ng_options %}
      {{ forms.select_multiple_ng_options(label, value) }}
    {% elseif value.autocomplete_portafolio %}
      {{ forms.autocomplete_portafolio(label, value) }}
    {% elseif value.password_to_confirm %}
      {{ forms.password_to_confirm(label, value, value.password_to_confirm) }}
