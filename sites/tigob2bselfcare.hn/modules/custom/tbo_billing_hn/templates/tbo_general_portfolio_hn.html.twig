              {% if data.with_icon %}
                <a href="" class="mask">
                  <i id="{{ data.with_icon.id }}" class="prefix {{ data.with_icon.icon }} {[{ classState }]}"
                    {% if data.with_icon.ng %} {{ data.with_icon.ng }}="{{ data.with_icon.value }}" {% endif %} >
                    <span class="path1"></span>
                    <span class="path2"></span>
                  </i>
                </a>
              {% endif %}

              <!--Progress bar-->
              {% if data.with_progress %} {{ forms.progress(data.with_progress.class, data.with_progress.style) }} {% endif %}

              <!--Status text-->
              {% if data.with_status %}
                <a id="{{ data.with_status.id }}">{{ data.with_status.angular }}</a>
              {% endif %}
